# LLM Configuration
MODEL_PROVIDER=gemini  # gemini 或 deepseek，根据你想使用的模型切换

# Gemini API配置
GEMINI_API_KEY=your_gemini_api_key_here

# DeepSeek API配置  
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_BASE_URL=https://api.deepseek.com

# Feature Switches
RAG_ENABLED=false
TOOLS_ENABLED=false

# Logging
LOG_LEVEL=INFO  # INFO 或 DEBUG

# LLM Parameters
MAX_TOKENS=4096
TEMPERATURE=0.7
TIMEOUT_SECONDS=30
MAX_RETRIES=3
